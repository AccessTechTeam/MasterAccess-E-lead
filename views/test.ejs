<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>test</title>
</head>

    <body>
        <h1>Rechercher des données JSON</h1>
        <form id="search-form">
          <label for="input">Entrez une recherche:</label>
          <input type="text" id="input" name="input">
          <button type="submit">Rechercher</button>
        </form>
        <div id="results"></div>
        <script>
          const form = document.getElementById('search-form');
          const resultsDiv = document.getElementById('results');
          form.addEventListener('submit', (event) => {
            event.preventDefault();
            const userInput = document.getElementById('input').value;
            fetch(`/donnees?input=${userInput}`)
              .then(response => response.json())
              .then(jsonData => {
                // utilisez jsonData pour afficher les résultats dans la page
                resultsDiv.textContent = JSON.stringify(jsonData);
                console.log("hdhd" , jsonData)
              });
          });
        </script>
      
</body>
</html>